ind_fit\r\n        real :: x\r\n        call find_fit(x, f)\r\n        contains\r\n        function f(z) result(y)\r\n            real, intent(in) :: z\r\n            real :: y\r\n        end function\r\n    end program\r\n```","reactions":{"url":"https://api.github.com/repos/lfortran/lfortran/issues/comments/1380137188/reactions","total_count":0,"+1":0,"-1":0,"laugh":0,"hooray":0,"confused":0,"heart":0,"rocket":0,"eyes":0},"performed_via_github_app":null,"event":"commented","actor":{"login":"Pranavchiku","id":85227306,"node_id":"MDQ6VXNlcjg1MjI3MzA2","avatar_url":"https://avatars.githubusercontent.com/u/85227306?v=4","gravatar_id":"","url":"https://api.github.com/users/Pranavchiku","html_url":"https://github.com/Pranavchiku","followers_url":"https://api.github.com/users/Pranavchiku/followers","following_url":"https://api.github.com/users/Pranavchiku/following{/other_user}","gists_url":"https://api.github.com/users/Pranavchiku/gists{/gist_id}","starred_url":"https://api.github.com/users/Pranavchiku/starred{/owner}{/repo}","subscriptions_url":"https://api.github.com/users/Pranavchiku/subscriptions","organizations_url":"https://api.github.com/users/Pranavchiku/orgs","repos_url":"https://api.github.com/users/Pranavchiku/repos","events_url":"https://api.github.com/users/Pranavchiku/events{/privacy}","received_events_url":"https://api.github.com/users/Pranavchiku/received_events","type":"User","site_admin":false}},{"url":"https://api.github.com/repos/lfortran/lfortran/issues/comments/1380141619","html_url":"https://github.com/lfortran/lfortran/issues/1163#issuecomment-1380141619","issue_url":"https://api.github.com/repos/lfortran/lfortran/issues/1163","id":1380141619,"node_id":"IC_kwDOCpMuHc5SQ0oz","user":{"login":"Pranavchiku","id":85227306,"node_id":"MDQ6VXNlcjg1MjI3MzA2","avatar_url":"https://avatars.githubusercontent.com/u/85227306?v=4","gravatar_id":"","url":"https://api.github.com/users/Pranavchiku","html_url":"https://github.com/Pranavchiku","followers_url":"https