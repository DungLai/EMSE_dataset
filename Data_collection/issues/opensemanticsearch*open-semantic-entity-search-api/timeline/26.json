 variants like\r\n`print(entity_manager.disjoin_chars(\"Georg Elser\"))`\r\n`['G e o r g E l s e r', 'Georg E l s e r']`\r\n","reactions":{"url":"https://api.github.com/repos/opensemanticsearch/open-semantic-entity-search-api/issues/comments/557793824/reactions","total_count":0,"+1":0,"-1":0,"laugh":0,"hooray":0,"confused":0,"heart":0,"rocket":0,"eyes":0},"performed_via_github_app":null,"event":"commented","actor":{"login":"Mandalka","id":1010780,"node_id":"MDQ6VXNlcjEwMTA3ODA=","avatar_url":"https://avatars.githubusercontent.com/u/1010780?v=4","gravatar_id":"","url":"https://api.github.com/users/Mandalka","html_url":"https://github.com/Mandalka","followers_url":"https://api.github.com/users/Mandalka/followers","following_url":"https://api.github.com/users/Mandalka/following{/other_user}","gists_url":"https://api.github.com/users/Mandalka/gists{/gist_id}","starred_url":"https://api.github.com/users/Mandalka/starred{/owner}{/repo}","subscriptions_url":"https://api.github.com/users/Mandalka/subscriptions","organizations_url":"https://api.github.com/users/Mandalka/orgs","repos_url":"https://api.github.com/users/Mandalka/repos","events_url":"https://api.github.com/users/Mandalka/events{/privacy}","received_events_url":"https://api.github.com/users/Mandalka/received_events","type":"User","site_admin":false}},{"id":2825319691,"node_id":"MDExOkNsb3NlZEV2ZW50MjgyNTMxOTY5MQ==","url":"https://api.github.com/repos/opensemanticsearch/open-semantic-entity-search-api/issues/events/2825319691","actor":{"login":"Mandalka","id":1010780,"node_id":"MDQ6VXNlcjEwMTA3ODA=","avatar_url":"https://avatars.githubusercontent.com/u/1010780?v=4","gravatar_id":"","url":"https://api.github.com/users/Mandalka","html_url":"https://github.com/Mandalka","followers_url":"https://api.github.com/users/Mandalka/followers","following_url":"https://api.github.com/users/Mandalka/following{/other_user}","gists_url":"https://api.github.com/users/Mandalka/gists{/gist_id}","starred_url":"https://api.github.com/