titem__(self, index):\r\n        image = self.images[index]\r\n        return self.pair(image)\r\n\r\n    def batches(self, batch_size=8):\r\n        images = self.images\r\n        while True:\r\n            batch_index = np.random.randint(len(images), size=batch_size)\r\n            x, y = zip(*[self.pair(self.images[index]) for index in batch_index])\r\n            x = np.stack(x, axis=0)  # Low Res\r\n            y = np.stack(y, axis=0)  # High Res\r\n            yield x, y`","reactions":{"url":"https://api.github.com/repos/wmylxmj/Anime-Super-Resolution/issues/comments/546421168/reactions","total_count":0,"+1":0,"-1":0,"laugh":0,"hooray":0,"confused":0,"heart":0,"rocket":0,"eyes":0},"performed_via_github_app":null,"event":"commented","actor":{"login":"Wulx2050","id":47112220,"node_id":"MDQ6VXNlcjQ3MTEyMjIw","avatar_url":"https://avatars.githubusercontent.com/u/47112220?v=4","gravatar_id":"","url":"https://api.github.com/users/Wulx2050","html_url":"https://github.com/Wulx2050","followers_url":"https://api.github.com/users/Wulx2050/followers","following_url":"https://api.github.com/users/Wulx2050/following{/other_user}","gists_url":"https://api.github.com/users/Wulx2050/gists{/gist_id}","starred_url":"https://api.github.com/users/Wulx2050/starred{/owner}{/repo}","subscriptions_url":"https://api.github.com/users/Wulx2050/subscriptions","organizations_url":"https://api.github.com/users/Wulx2050/orgs","repos_url":"https://api.github.com/users/Wulx2050/repos","events_url":"https://api.github.com/users/Wulx2050/events{/privacy}","received_events_url":"https://api.github.com/users/Wulx2050/received_events","type":"User","site_admin":false}},{"url":"https://api.github.com/repos/wmylxmj/Anime-Super-Resolution/issues/comments/551059586","html_url":"https://github.com/wmylxmj/Anime-Super-Resolution/issues/5#issuecomment-551059586","issue_url":"https://api.github.com/repos/wmylxmj/Anime-Super-Resolution/issues/5","id":551059586,"node_id":"MDEyOklzc3VlQ29tbWVudDU1MTA1OTU4Ng=