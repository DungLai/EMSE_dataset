ect_pad = True\r\n                self.func = keras.layers.Conv2D (*args, **kwargs)\r\n\r\n            def __call__(self,x):\r\n                if self.reflect_pad:\r\n                    x = ReflectionPadding2D( self.pad ) (x)\r\n                return self.func(x)\r\nnnlib.Conv2D = Conv2D\r\n","reactions":{"url":"https://api.github.com/repos/iperov/DeepFaceLab/issues/comments/483592830/reactions","total_count":0,"+1":0,"-1":0,"laugh":0,"hooray":0,"confused":0,"heart":0,"rocket":0,"eyes":0},"performed_via_github_app":null,"event":"commented","actor":{"login":"staxoverflowed","id":49677498,"node_id":"MDQ6VXNlcjQ5Njc3NDk4","avatar_url":"https://avatars.githubusercontent.com/u/49677498?v=4","gravatar_id":"","url":"https://api.github.com/users/staxoverflowed","html_url":"https://github.com/staxoverflowed","followers_url":"https://api.github.com/users/staxoverflowed/followers","following_url":"https://api.github.com/users/staxoverflowed/following{/other_user}","gists_url":"https://api.github.com/users/staxoverflowed/gists{/gist_id}","starred_url":"https://api.github.com/users/staxoverflowed/starred{/owner}{/repo}","subscriptions_url":"https://api.github.com/users/staxoverflowed/subscriptions","organizations_url":"https://api.github.com/users/staxoverflowed/orgs","repos_url":"https://api.github.com/users/staxoverflowed/repos","events_url":"https://api.github.com/users/staxoverflowed/events{/privacy}","received_events_url":"https://api.github.com/users/staxoverflowed/received_events","type":"User","site_admin":false}},{"id":2279894570,"node_id":"MDExOkNsb3NlZEV2ZW50MjI3OTg5NDU3MA==","url":"https://api.github.com/repos/iperov/DeepFaceLab/issues/events/2279894570","actor":{"login":"staxoverflowed","id":49677498,"node_id":"MDQ6VXNlcjQ5Njc3NDk4","avatar_url":"https://avatars.githubusercontent.com/u/49677498?v=4","gravatar_id":"","url":"https://api.github.com/users/staxoverflowed","html_url":"https://github.com/staxoverflowed","followers_url":"https://api.github.com/users/stax