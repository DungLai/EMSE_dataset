_url":"https://api.github.com/users/JiachuanDENG/repos","events_url":"https://api.github.com/users/JiachuanDENG/events{/privacy}","received_events_url":"https://api.github.com/users/JiachuanDENG/received_events","type":"User","site_admin":false},"created_at":"2019-01-28T18:22:44Z","updated_at":"2019-01-28T18:22:44Z","author_association":"NONE","body":"Thanks for your clarification. I have no doubt on this question now :)","reactions":{"url":"https://api.github.com/repos/mravanelli/SincNet/issues/comments/458244432/reactions","total_count":0,"+1":0,"-1":0,"laugh":0,"hooray":0,"confused":0,"heart":0,"rocket":0,"eyes":0},"performed_via_github_app":null,"event":"commented","actor":{"login":"JiachuanDENG","id":20760190,"node_id":"MDQ6VXNlcjIwNzYwMTkw","avatar_url":"https://avatars.githubusercontent.com/u/20760190?v=4","gravatar_id":"","url":"https://api.github.com/users/JiachuanDENG","html_url":"https://github.com/JiachuanDENG","followers_url":"https://api.github.com/users/JiachuanDENG/followers","following_url":"https://api.github.com/users/JiachuanDENG/following{/other_user}","gists_url":"https://api.github.com/users/JiachuanDENG/gists{/gist_id}","starred_url":"https://api.github.com/users/JiachuanDENG/starred{/owner}{/repo}","subscriptions_url":"https://api.github.com/users/JiachuanDENG/subscriptions","organizations_url":"https://api.github.com/users/JiachuanDENG/orgs","repos_url":"https://api.github.com/users/JiachuanDENG/repos","events_url":"https://api.github.com/users/JiachuanDENG/events{/privacy}","received_events_url":"https://api.github.com/users/JiachuanDENG/received_events","type":"User","site_admin":false}},{"id":2114023315,"node_id":"MDExOkNsb3NlZEV2ZW50MjExNDAyMzMxNQ==","url":"https://api.github.com/repos/mravanelli/SincNet/issues/events/2114023315","actor":{"login":"mravanelli","id":16886998,"node_id":"MDQ6VXNlcjE2ODg2OTk4","avatar_url":"https://avatars.githubusercontent.com/u/16886998?v=4","gravatar_id":"","url":"https://api.github.com/users/mravanelli",