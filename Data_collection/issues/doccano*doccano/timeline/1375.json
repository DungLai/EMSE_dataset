unning well.\r\n![image](https://user-images.githubusercontent.com/10695171/118943390-25734680-b986-11eb-8dd1-77a5764b8a43.png)\r\n\r\n\r\n\r\n","reactions":{"url":"https://api.github.com/repos/doccano/doccano/issues/comments/844844888/reactions","total_count":0,"+1":0,"-1":0,"laugh":0,"hooray":0,"confused":0,"heart":0,"rocket":0,"eyes":0},"performed_via_github_app":null,"event":"commented","actor":{"login":"hushulong","id":10695171,"node_id":"MDQ6VXNlcjEwNjk1MTcx","avatar_url":"https://avatars.githubusercontent.com/u/10695171?v=4","gravatar_id":"","url":"https://api.github.com/users/hushulong","html_url":"https://github.com/hushulong","followers_url":"https://api.github.com/users/hushulong/followers","following_url":"https://api.github.com/users/hushulong/following{/other_user}","gists_url":"https://api.github.com/users/hushulong/gists{/gist_id}","starred_url":"https://api.github.com/users/hushulong/starred{/owner}{/repo}","subscriptions_url":"https://api.github.com/users/hushulong/subscriptions","organizations_url":"https://api.github.com/users/hushulong/orgs","repos_url":"https://api.github.com/users/hushulong/repos","events_url":"https://api.github.com/users/hushulong/events{/privacy}","received_events_url":"https://api.github.com/users/hushulong/received_events","type":"User","site_admin":false}},{"id":4789554633,"node_id":"MDExOkNsb3NlZEV2ZW50NDc4OTU1NDYzMw==","url":"https://api.github.com/repos/doccano/doccano/issues/events/4789554633","actor":{"login":"Hironsan","id":6737785,"node_id":"MDQ6VXNlcjY3Mzc3ODU=","avatar_url":"https://avatars.githubusercontent.com/u/6737785?v=4","gravatar_id":"","url":"https://api.github.com/users/Hironsan","html_url":"https://github.com/Hironsan","followers_url":"https://api.github.com/users/Hironsan/followers","following_url":"https://api.github.com/users/Hironsan/following{/other_user}","gists_url":"https://api.github.com/users/Hironsan/gists{/gist_id}","starred_url":"https://api.github.com/users/Hironsan/starred{/owner}{/repo}