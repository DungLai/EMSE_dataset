 useful. ","reactions":{"url":"https://api.github.com/repos/doccano/doccano/issues/comments/754739023/reactions","total_count":3,"+1":3,"-1":0,"laugh":0,"hooray":0,"confused":0,"heart":0,"rocket":0,"eyes":0},"performed_via_github_app":null,"event":"commented","actor":{"login":"Simaex","id":33528038,"node_id":"MDQ6VXNlcjMzNTI4MDM4","avatar_url":"https://avatars.githubusercontent.com/u/33528038?v=4","gravatar_id":"","url":"https://api.github.com/users/Simaex","html_url":"https://github.com/Simaex","followers_url":"https://api.github.com/users/Simaex/followers","following_url":"https://api.github.com/users/Simaex/following{/other_user}","gists_url":"https://api.github.com/users/Simaex/gists{/gist_id}","starred_url":"https://api.github.com/users/Simaex/starred{/owner}{/repo}","subscriptions_url":"https://api.github.com/users/Simaex/subscriptions","organizations_url":"https://api.github.com/users/Simaex/orgs","repos_url":"https://api.github.com/users/Simaex/repos","events_url":"https://api.github.com/users/Simaex/events{/privacy}","received_events_url":"https://api.github.com/users/Simaex/received_events","type":"User","site_admin":false}},{"id":4170674332,"node_id":"MDE0Ok1lbnRpb25lZEV2ZW50NDE3MDY3NDMzMg==","url":"https://api.github.com/repos/doccano/doccano/issues/events/4170674332","actor":{"login":"Hironsan","id":6737785,"node_id":"MDQ6VXNlcjY3Mzc3ODU=","avatar_url":"https://avatars.githubusercontent.com/u/6737785?v=4","gravatar_id":"","url":"https://api.github.com/users/Hironsan","html_url":"https://github.com/Hironsan","followers_url":"https://api.github.com/users/Hironsan/followers","following_url":"https://api.github.com/users/Hironsan/following{/other_user}","gists_url":"https://api.github.com/users/Hironsan/gists{/gist_id}","starred_url":"https://api.github.com/users/Hironsan/starred{/owner}{/repo}","subscriptions_url":"https://api.github.com/users/Hironsan/subscriptions","organizations_url":"https://api.github.com/users/Hironsan/orgs","repos_url":"https://api.